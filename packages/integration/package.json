  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts  test",
    "test:fix": "nvm use v13.0.0",
    "test:match": "react-scripts  test --env=jsdom  -i useloginform -t \"math tests\"",
    "test:nowatch": "react-scripts test --env=jsdom --watchAll=false",
    "test:api_endpoints": "yarn test src/tests/jest/Api_routes.test.tsx  --env=jsdom --watchAll=false",
    "eject": "react-scripts  eject",
    "test-coverage": "sh ./.test-coverage.sh",
    "sonar": "sonar-scanner",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "lint": " yarn run eslint  src/**/*.ts  src/**/*.tsx   --ext=.tsx,.ts --fix  --ignore-path .eslintignore",
    "lint:circular": "madge --circular ./src/index.tsx",
    "build:doc": "typedoc --out ./docs/documentation ./src   --tsconfig ./tsconfig.json",
    "check-licence": " license-checker | grep 'licenses:'  | sort -u",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "cypress:open": "cypress open  --config integrationFolder=cypress/tests_integration,video=false,defaultCommandTimeout=10000",
    "lint:css": "stylelint src/**/*.style.tsx",
    "postman:run": "sh postman.sh",
    "cypress:run": "yarn cypress run  --config integrationFolder=cypress/tests_integration,video=false,defaultCommandTimeout=10000"
  },
